{
  "name": "Twilight",
  "type": "dark",
  "colors": {
    "editor.background": "#141414",
    "editor.foreground": "#F8F8F8",
    "editor.lineHighlightBackground": "#FFFFFF08",
    "editor.selectionBackground": "#DDF0FF33",
    "editor.selectionHighlightBackground": "#DDF0FF20",
    "editor.findMatchBackground": "#DDF0FF40",
    "editor.findMatchHighlightBackground": "#DDF0FF20",
    "editor.wordHighlightBackground": "#DDF0FF20",
    "editor.wordHighlightStrongBackground": "#DDF0FF30",
    "editorCursor.foreground": "#A7A7A7",
    "editorWhitespace.foreground": "#FFFFFF40",
    "editorIndentGuide.background": "#FFFFFF20",
    "editorIndentGuide.activeBackground": "#FFFFFF40",
    "editorLineNumber.foreground": "#5F5A60",
    "editorLineNumber.activeForeground": "#A7A7A7",
    "editorGutter.background": "#141414",
    "editorGutter.modifiedBackground": "#F9EE98",
    "editorGutter.addedBackground": "#7FD987",
    "editorGutter.deletedBackground": "#F97A7A",
    "sideBar.background": "#1A1A1A",
    "sideBar.foreground": "#F8F8F8",
    "sideBarSectionHeader.background": "#2A2A2A",
    "sideBarSectionHeader.foreground": "#F8F8F8",
    "activityBar.background": "#0F0F0F",
    "activityBar.foreground": "#F8F8F8",
    "activityBar.inactiveForeground": "#5F5A60",
    "statusBar.background": "#0F0F0F",
    "statusBar.foreground": "#F8F8F8",
    "statusBar.noFolderBackground": "#0F0F0F",
    "titleBar.activeBackground": "#0F0F0F",
    "titleBar.activeForeground": "#F8F8F8",
    "titleBar.inactiveBackground": "#0A0A0A",
    "titleBar.inactiveForeground": "#5F5A60",
    "tab.activeBackground": "#141414",
    "tab.activeForeground": "#F8F8F8",
    "tab.inactiveBackground": "#0F0F0F",
    "tab.inactiveForeground": "#5F5A60",
    "tab.border": "#2A2A2A",
    "panel.background": "#141414",
    "panel.border": "#2A2A2A",
    "panelTitle.activeForeground": "#F8F8F8",
    "panelTitle.inactiveForeground": "#5F5A60",
    "terminal.background": "#141414",
    "terminal.foreground": "#F8F8F8",
    "menu.background": "#1A1A1A",
    "menu.foreground": "#F8F8F8",
    "dropdown.background": "#1A1A1A",
    "dropdown.foreground": "#F8F8F8",
    "dropdown.border": "#2A2A2A",
    "input.background": "#1A1A1A",
    "input.foreground": "#F8F8F8",
    "input.border": "#2A2A2A",
    "inputOption.activeBorder": "#DDF0FF",
    "scrollbar.shadow": "#00000050",
    "scrollbarSlider.background": "#FFFFFF20",
    "scrollbarSlider.hoverBackground": "#FFFFFF30",
    "scrollbarSlider.activeBackground": "#FFFFFF40"
  },
  "tokenColors": [
    {
      "name": "Commons",
      "scope": [
        "punctuation.separator",

        "meta.function-call entity.name.function",
        "meta.method-call entity.name.function",

        "meta.object-literal.key",
        "meta.property-value",
        "variable.other.object.property"
      ],
      "settings": {
        "foreground": "#F8F8F8"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#5F5A60"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#CDA869"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted"
      ],
      "settings": {
        "foreground": "#8F9D6A"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#CF6A4C"
      }
    },
    {
      "name": "Definition",
      "scope": [
        "entity.name.function",
        "meta.module entity.name.type",
        "meta.class entity.name.type"
      ],
      "settings": {
        "foreground": "#9B703F"
      }
    },
    {
      "name": "Built-in Functions",
      "scope": [
        "support.function",
        "support.function.builtin",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#F9EE98"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.name.type",
        "support.type",
        "support.class",
        "meta.class",
        "new.expr meta.function-call entity.name.function"
      ],
      "settings": {
        "foreground": "#9B859D"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#7587A6"
      }
    },
    {
      "name": "Tag Attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#F9EE98"
      }
    },
    {
      "name": "Diff Header",
      "scope": [
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "background": "#0E2231",
        "foreground": "#F8F8F8"
      }
    },
    {
      "name": "Diff Deleted",
      "scope": "markup.deleted",
      "settings": {
        "background": "#420E09",
        "foreground": "#F8F8F8"
      }
    },
    {
      "name": "Diff Inserted",
      "scope": "markup.inserted",
      "settings": {
        "background": "#253B22",
        "foreground": "#F8F8F8"
      }
    },
    {
      "name": "Diff Changed",
      "scope": "markup.changed",
      "settings": {
        "background": "#4A410D",
        "foreground": "#F8F8F8"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#E9C062"
      }
    },
    {
      "name": "Escape Characters",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#CF6A4C"
      }
    },
    {
      "name": "Embedded",
      "scope": [
        "punctuation.section.embedded",
        "variable.interpolation"
      ],
      "settings": {
        "foreground": "#DAEFA3"
      }
    },
    {
      "name": "Built-in constant",
      "scope": [
        "constant.language",
        "support.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#CF6A4C"
      }
    },
    {
      "name": "Support",
      "scope": "support",
      "settings": {
        "foreground": "#9B703F"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#F9EE98"
      }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#CDA869"
      }
    },
    {
      "name": "JSON Key - Level 2",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#9B859D"
      }
    }
  ]
}
