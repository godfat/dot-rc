{
  "name": "twilight",
  "displayName": "Twilight",
  "description": "Classic Twilight dark theme for VS Code",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": ["Themes", "Snippets"],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./main.js",
  "contributes": {
    "themes": [
      {
        "label": "Twilight",
        "uiTheme": "vs-dark",
        "path": "./twilight.json"
      }
    ],
    "snippets": [
      {
        "language": "ruby",
        "path": "./snippets-ruby.json"
      }
    ],
    "keybindings": [
      {
        "command": "twilight.compltetion.cycle",
        "key": "escape",
        "when": "editorTextFocus && !editorHasMultipleSelections && !inDiffEditor"
      }
    ],
    "commands": [
      {
        "command": "twilight.compltetion.cycle",
        "title": "Text complete",
        "category": "Twilight"
      },
      {
        "command": "twilight.paste",
        "title": "Indent paste",
        "category": "Twilight"
      }
    ]
  }
}
