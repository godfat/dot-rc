{
  "name": "twilight",
  "displayName": "Twilight",
  "description": "Classic Twilight dark theme for VS Code",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": ["Themes", "Snippets"],
  "main": "./text-completion",
  "contributes": {
    "themes": [
      {
        "label": "Twilight",
        "uiTheme": "vs-dark",
        "path": "./twilight.json"
      }
    ],
    "snippets": [
      {
        "language": "ruby",
        "path": "./snippets-ruby.json"
      }
    ],
    "keybindings": [
      {
        "command": "twilightTextCompletion.cycle",
        "key": "escape",
        "when": "editorTextFocus && !editorHasMultiSelection && !editorReadOnly"
      }
    ]
  }
}
