{
  "breadcrumbs.enabled": false,
  "editor.bracketPairColorization.enabled": false,
  "editor.find.seedSearchStringFromSelection": "never",
  "editor.fontFamily": "'DejaVu Sans Mono', Menlo, Monaco, monospace",
  "editor.hideCursorInOverviewRuler": true,
  "editor.hover.enabled": false,
  "editor.inlineSuggest.enabled": false,
  "editor.lightbulb.enabled": "off",
  "editor.minimap.enabled": false,
  "editor.occurrencesHighlight": "off",
  "editor.parameterHints.enabled": false,
  "editor.quickSuggestions": {
    "comments": "off",
    "strings": "off",
    "other": "off"
  },
  "editor.renderWhitespace": "all",
  "editor.rulers": [78],
  "editor.scrollBeyondLastLine": false,
  "editor.stickyScroll.enabled": false,
  "editor.suggestOnTriggerCharacters": false,
  "editor.tabCompletion": "onlySnippets",
  "editor.tabSize": 2,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "editor.wordWrap": "wordWrapColumn",
  "[markdown]": {
    "editor.wordWrap": "wordWrapColumn"
  },
  "editor.wrappingIndent": "none",
  "extensions.ignoreRecommendations": true,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.openRepositoryInParentFolders": "never",
  "scm.diffDecorations": "gutter",
  "scm.diffDecorationsGutterPattern": {
    "modified": false
  },
  "search.searchEditor.defaultNumberOfContextLines": 0,
  "search.searchEditor.reusePriorSearchConfiguration": true,
  "search.searchOnType": false,
  "search.useIgnoreFiles": false,
  "security.workspace.trust.enabled": false,
  "task.allowAutomaticTasks": "off",
  "telemetry.telemetryLevel": "off",
  "update.mode": "start",
  "window.commandCenter": false,
  "window.density.editorTabHeight": "compact",
  "window.zoomLevel": 3,
  "window.zoomPerWindow": false,
  "workbench.activityBar.location": "hidden",
  "workbench.colorTheme": "Twilight",
  "workbench.editor.closeEmptyGroups": false,
  "workbench.editor.doubleClickTabToToggleEditorGroupSizes": "off",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.splitOnDragAndDrop": false,
  "workbench.editor.tabActionCloseVisibility": false,
  "workbench.layoutControl.enabled": false,
  "workbench.navigationControl.enabled": false,
  "workbench.startupEditor": "none"
}
